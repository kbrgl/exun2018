{
  "extends": ["airbnb", "prettier"],
  "rules": {
    "semi": ["warn", "never"],
    "no-shadow": "off",
    "no-param-reassign": "off",
    "no-use-before-define": "off",
    "import/no-extraneous-dependencies": "off",
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off"
  },
  "plugins": [
    "flowtype"
  ],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "jquery": true
  }
}
